pipeline {
    agent any 
    stages {
        stage('clone repo') { 
            steps {
               bat "git clone https://github.com/mounika-93/pipeline.git"
               bat "mvn clean"
            }
        }
        stage('Test') { 
            steps {
               bat "mvn test"
            }
        }
        stage('Deploy') { 
            steps {
             bat "mvn package"
            }
        }
    }
}
