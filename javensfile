pipeline {
    agent any 
    stages {
        stage('clone repo') { 
            steps {
               sh "git clone https://github.com/mounika-93/pipeline.git"
               sh "mvn clean"
            }
        }
        stage('Test') { 
            steps {
               sh "mvn test"
            }
        }
        stage('Deploy') { 
            steps {
             sh "mvn package"
            }
        }
    }
}
